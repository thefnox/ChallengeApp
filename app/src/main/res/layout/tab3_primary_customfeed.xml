    android:layout_height="match_parent"
    android:paddingLeft="6dp"
    android:paddingRight="6dp"
    android:paddingTop="1dp"
    tools:context="team10.hkr.challengeapp.Controllers.PrimaryActivity$PlaceholderFragment">
